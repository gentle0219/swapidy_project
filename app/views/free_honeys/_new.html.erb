<img id="bg" src="/images/login_background.jpg" style="width: 1905px; height: 1562px; display: inline;">
<div id="login_container" style="position:fixed; top:300px;">
  <img id="login_circle" src="/images/login_circle.png" style="top: 338.5px; display: block;">
  
  <div id="login_form">
    <%= form_for(FreeHoney.new, :as => "free_honey", :url => "/free_honeys/create") do |f| %>
      <div><label for="emails">Invite Emails</label><br />
      <input id="emails" name="emails" size="30" type="text" value="" /></div>
    
      <div style="position:relative; top:10px;">
        <input name="Submit" type="submit" value="Invite" />
        <a href="javascript:hidePopup();" style="color: black; padding-left: 30px;">Cancel</a>
      </div>
    <% end %>
  </div>
</div>