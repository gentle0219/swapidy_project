$("#shopping_cart").html("<%= raw escape_javascript(@return_content[0]) %>");
$("#drop_items").html("<%= raw escape_javascript(@return_content[1]) %>");
<% unless ShoppingCart.cart_products_empty? %>
  $("#shopping_cart").show();
  <% if @changed_type && @changed_type == :sell %>
	  $("#product_items .sell_display").hide();
	  $("#product_items .sell_display").fadeIn('slow');
  <% elsif @changed_type && @changed_type == :buy %>
	  $("#product_items .buy_display").hide();
	  $("#product_items .buy_display").fadeIn('slow');
	    $(".show_result").show();
  <% end %>
  display_checkout_sliders();
<% end %>
if(for_what == "for_buying"){
  $('#checkout_button').show();
  $('#done_button').hide();
}
